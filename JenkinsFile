pipeline {
  agent {
  node{
  label 'master'
  }
  }
  tools {
    maven 'MAVEN_HOME'
  }
  stages{
    stage('build'){
    steps{
        sh 'mvn -version \
           cd MyApplication '
          sh 'mvn compile package'
        }
      }
    }
    post{
    always{
      archive 'target/*.jar'
    }
    }
  }
